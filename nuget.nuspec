<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <version>3.0.0</version>
        <id>datatables.net-select-jqui</id>
        <description>This is Select for DataTables with styling for [jQueryUI](http://jqueryui.com/)</description>
        <repository type="git" url="https://github.com/DataTables/Dist-DataTables-Select-jQueryUI.git"/>
        <authors>SpryMedia Ltd</authors>
        <projectUrl>http://datatables.net</projectUrl>
        <license type="expression">MIT</license>
        <tags>select  selection  jQuery UI  jQueryUI  Datatables jQuery table</tags>
        <dependencies>
            <dependency id="datatables.net-select" version="1.4.0"/>
            <dependency id="datatables.net-jqui" version="1.12.1"/>
        </dependencies>
        <contentFiles>
            <files include="**/*" buildAction="Content"/>
        </contentFiles>
    </metadata>
    <files>
        <file src="js/select.jqueryui.js" target="content\Scripts"/>
        <file src="js/select.jqueryui.js" target="contentFiles\any\any\wwwroot\js"/>
        <file src="js/select.jqueryui.min.js" target="content\Scripts"/>
        <file src="js/select.jqueryui.min.js" target="contentFiles\any\any\wwwroot\js"/>
        <file src="css/select.jqueryui.css" target="content\Content"/>
        <file src="css/select.jqueryui.css" target="contentFiles\any\any\wwwroot\css"/>
        <file src="css/select.jqueryui.min.css" target="content\Content"/>
        <file src="css/select.jqueryui.min.css" target="contentFiles\any\any\wwwroot\css"/>
    </files>
</package>

